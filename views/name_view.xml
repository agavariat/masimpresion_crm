<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="view_partner_simple_form_inheritted" model="ir.ui.view">
            <field name="name">res.partner.simplified.form.inheritted</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='vat']" position="after">
                    <field name="tamemp"/>
                    <field name="linea" widget="many2many_tags"/>
                    <field name="com"/>
                </xpath>
                <xpath expr="//field[@name='website']" position="after">
                    <field name="sec"/>
                    <field name="client"/>
                </xpath>
            </field>
       </record>
       <record id="crm_case_form_view_oppor_inherit_product" model="ir.ui.view">
            <field name="name">crm.lead.oppor.inherited</field>
            <field name="model">crm.lead</field>
            <field name="inherit_id" ref="crm.crm_lead_view_form" />
            <field name="arch" type="xml">  
                <xpath expr="//page/field[@name='website']" position="inside">
                    <field name="tamemp"/>
                    <field name="linea" widget="many2many_tags"/>
                    <field name="com"/>
                </xpath>
                <!--xpath expr="//field[@name='website']" position="after">
                    <field name="sec"/>
                    <field name="client"/>
                </xpath-->
            </field>
       </record>
       
    </data>
</odoo>